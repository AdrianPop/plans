{
    "name": "ipop/plans",
    "description": "Laravel Plans is a package for SaaS apps that need management over plans, features, subscriptions, events for plans or limited, countable features.",
    "keywords": [
        "eloquent",
        "laravel",
        "subscription",
        "plan",
        "plans",
        "model",
        "saas"
    ],
    "homepage": "https://github.com/rennokki/plans",
    "license": "MIT",
    "authors": [
        {
            "name": "Alex Renoki",
            "email": "rennokki@gmail.com",
            "homepage": "https://twitter.com/rennokki",
            "role": "Developer"
        },
        {
            "name": "Vinicius V.",
            "email": "vinyvicente@gmail.com"
        },
        {
            "name": "Adrian Pop",
            "email": "ipop@eml.cc"
        }
    ],
    "require": {
        "php": "^8.0|^8.1",
        "ext-dom": "*",
        "ext-json": "*",
        "ext-openssl": "*",
        "ext-soap": "*",
        "doctrine/dbal": "^3.0",
        "laravel/framework": "~9"
    },
    "require-dev": {
        "laravel/legacy-factories": "^1.1",
        "orchestra/testbench": "^7.0|^6.6",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Rennokki\\Plans\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rennokki\\Plans\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Rennokki\\Plans\\PlansServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable"
}
